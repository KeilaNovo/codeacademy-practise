from datetime import datetime

now=datetime.now()
print now



from datetime import datetime
now = datetime.now()

print '%s/%s/%s' % (now.month,now.day,now.year)



from datetime import datetime
now = datetime.now()

print '%s:%s:%s' % (now.hour,now.minute,now.second)


from datetime import datetime
now = datetime.now()


print '%s/%s/%s %s:%s:%s' % (now.month, now.day, now.year, now.hour, now.minute, now.second)

